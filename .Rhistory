plot_gnomv2_r <- function() {
# Read the data from the test.csv file
data <- read.csv("test.csv", header = TRUE)
# Read the data from the test2.csv file
data2 <- read.csv("test2.csv", header = TRUE)
# Assign the values to the variables
plot1 <- ggplot(data, aes(x = as.factor(r_0), y = j_0)) +
geom_bar(stat = "identity", fill = "blue") +
labs(x = "εNd Released from Sediment", y = "Total Flux (pmol/yr)") +
ggtitle("Av. Seafloor Sediment flux, (u)") +
scale_y_continuous(labels = function(x) sprintf("%s", x)) +
theme_light()
plot2 <- ggplot(data, aes(x = as.factor(r_0), y = a_0)) +
geom_bar(stat = "identity", fill = "orange") +
labs(x = "Sediment Flux εNd", y = "Total Sediment Area (u)") +
ggtitle("Av. Seafloor Sediment flux, (u)") +
scale_y_continuous(labels = function(x) sprintf("%s", x)) +
theme_light()
plot3 <- ggplot(data2, aes(x = as.factor(r_0), y = m_0)) +
geom_bar(stat = "identity", fill = "green") +
labs(x = "m_0", y = "Total m_0") +
ggtitle("Av. Seafloor Sediment flux, (u)") +
scale_y_continuous(labels = function(x) sprintf("%s", x)) +
theme_light()
plot4 <- ggplot(data2, aes(x = as.factor(r_0), y = n_0)) +
geom_bar(stat = "identity", fill = "red") +
labs(x = "n_0", y = "Total n_0") +
ggtitle("Av. Seafloor Sediment flux, (u)") +
scale_y_continuous(labels = function(x) sprintf("%s", x)) +
theme_light()
# Arrange the plots side by side
grid.arrange(plot1, plot2, plot3, plot4, ncol = 2)
}
